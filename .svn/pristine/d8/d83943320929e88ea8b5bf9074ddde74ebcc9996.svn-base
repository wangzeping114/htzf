h2 .button, h3 .button, h4 .button { float: right; margin-right: 10px; margin-bottom: 2px; }
a{cursor:pointer;}
a:hover,a:focus{
	outline: none;
	-moz-outline:none;
}
a {blr:expression_r(this.onFocus=this.blur())} 

.field label.text { position: absolute; top: 7px; }

.img-view-box { max-width: 200px; max-height: 200px; }
.img-view-box img { max-width: 100%; max-height: 100%; cursor: pointer; }

.table-imglist { width: 780px !important; border-left: solid 1px #ddd; border-top: solid 1px #ddd; }
.table-imglist td { width: 195px; height: 195px; line-height: 195px; text-align: center; vertical-align: middle; padding: 5px; 
                    border-right: solid 1px #ddd; border-bottom: solid 1px #ddd; }
.table-imglist td img { max-width: 100%; max-height: 100%; }
.table-imglist td img.toadd { cursor: pointer; }

ul.img-multilist { padding: 0; width: 780px; border-left: solid 1px #ddd; }
.img-multilist .img-multi-item { width: 194px; height: 194px; float: left; border-right: solid 1px #ddd; border-bottom: solid 1px #ddd; position: relative; }
.img-multilist .border-top { border-top: solid 1px #ddd; }
.img-multi-item:hover a { display: block; }
.img-multi-item a { position: absolute; left: 0; top: 0; z-index: 5; height: 30px; line-height: 30px; text-align: center; background-color: #f5f5f5; display: none; }
.img-multi-item table { position: absolute; left: 0; top: 0; z-index: 1; width: 194px; height: 194px; padding: 0; margin: 0; list-style: none; }
.img-multi-item table tr td { width: 100%; height: 100%; text-align: center; vertical-align: middle; padding: 5px; }
.img-multi-item img { max-width: 184px; max-height: 184px; *width:expression(this.width>184?"184":true); *width:expression(this.height>184?"184":true); }
.img-multi-item img.toadd { cursor: pointer; }

.page-box { text-align: center; margin: 10px 0; }

.calc-box { width: 20px; position: absolute; top: 0; right: 20px; bottom: 0; text-align: center; cursor: pointer; display: none; }
.calc-box .calc-item { height: 50%; background-image:url(../images/sort.png); background-repeat: no-repeat; }
.calc-box .sort-up { background-position: -27px -3px; }
.calc-box .sort-down { background-position: -3px -3px; }

.table { table-layout: fixed; }
.table th, .table td { word-wrap: break-word; }

